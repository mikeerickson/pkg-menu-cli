  "keywords": [
    "package",
    "menu",
    "script",
    "cli"
  ],
  "author": "Mike Erickson, codedungeon@gmail.com",
  "license": "MIT",
  "reveal": true,
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-table2": "^0.2.0",
    "execa": "^0.5.0",
    "meow": "^3.7.0",
    "update-notifier": "^1.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeerickson/pkg-menu-cli.git"
  },
  "bugs": {
    "url": "https://github.com/mikeerickson/pkg-menu-cli/issues"
