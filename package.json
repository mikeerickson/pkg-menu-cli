{
  "name": "package-menu-cli",
  "version": "1.2.2",
  "description": "Displays menu of scripts from directory containing `package.json`",
  "main": "cli.js",
  "bin": {
    "package-menu": "cli.js"
  },
  "scripts": {
    "eslint": "eslint \"./**/*.js\"",
    "test": "mocha",
    "menu": "node ./cli.js",
